{
    "plugins": {
        "heading-increment": {
            "enabled": false
        },
        "heading-style": {
            "style": "atx"
        },
        "ul-style": {
            "style": "asterisk"
        },
        "line-length": {
            "enabled": false
        },
        "no-duplicate-heading": {
            "enabled": false
        },
        "single-title": {
            "enabled": false
        },
        "no-trailing-punctuation": {
            "enabled": false
        },
        "no-blanks-blockquote": {
            "enabled": false
        },
        "hr-style": {
            "style": "* * *"
        },
        "first-line-heading": {
            "enabled": false
        },
        "no-alt-text": {
            "enabled": false
        },
        "code-block-style": {
            "style": "fenced"
        },
        "code-fence-style": {
            "style": "backtick"
        }
    }
}
